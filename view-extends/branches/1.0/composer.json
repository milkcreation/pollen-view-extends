{
  "name": "pollen-solutions/view-extends",
  "description": "Pollen View Extends Component - Pollen view component extended functionality.",
  "homepage": "https://www.presstify.com/pollen-solutions/view-extends/",
  "keywords": [
    "pollen-solutions",
    "component",
    "view",
    "extend",
    "functions"
  ],
  "time": "2021-07-02",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "pollen-solutions/event": "^1.0",
    "pollen-solutions/view": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\ViewExtends\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\ViewExtends\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
